<div id="messageContainer">
  <div id="messageInfoContainer">
    <div id="nameContainer">
      <div id="userName"><%= username %></div>
    </div>
    <div id="dateContainer">
      <div id="time"><%= time %></div>
      <div id="date"><%= date %></div>
    </div>
  </div>
  <div id="textContainer">
    <%= message %>
  </div>
</div>